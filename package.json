    "log4js": "^4.1.0",
    "md5": "^2.2.1",
    "mongoose": "^5.4.20",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "shortid": "^2.2.14",
    "stream-to-buffer": "^0.1.0",
    "bluebird": "^3.5.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.2"
